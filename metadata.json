{
  "name": "plexxi-debnet",
  "version": "2.0.0",
  "author": "T. Repasi",
  "summary": "Puppet module to manage /etc/network/interfaces.d/* files on Debian based Linux systems.",
  "tags": [
    "network",
    "interfaces",
    "ifup",
    "ifdown",
    "bridge",
    "bonding"
  ],
  "license": "Apache-2.0",
  "project_page": "https://github.com/Plexxi/plexxi-debnet.git",
  "source": "https://github.com/Plexxi/plexxi-debnet.git",
  "issues_url": "https://github.com/Plexxi/plexxi-debnet/issues",
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "6",
        "7",
        "8"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "12.04",
        "14.04"
      ]
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs/concat",
      "version_requirement": ">= 2.0.0 < 3.0.0"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.12.0 < 5.0.0"
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">=3.0.0 <5.0.0"
    }
  ]
}
